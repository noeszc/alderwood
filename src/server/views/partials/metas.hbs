{{#if facebookData}}
  <meta property="og:type" content="article" />
  <meta property="og:title" content="{{facebookData.title}}" />
  <meta property="og:description" content="{{facebookData.large_description}}" />
  <meta property="og:image" content="{{facebookData.image_medium}}" />
  <meta property="fb:app_id" content="{{facebookData.facebookId}}" />
{{/if}}